    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "8.2.1",
    "babel-plugin-named-asset-import": "^0.3.7",
    "babel-preset-react-app": "^10.0.0",
    "camelcase": "^6.2.0",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "css-loader": "5.0.1",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
